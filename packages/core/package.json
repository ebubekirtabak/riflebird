{
  "name": "@riflebird/core",
  "version": "0.2.0",
  "private": true,
  "description": "AI-powered all in one test generation and execution framework",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "jiti": "^2.6.1",
    "openai": "^4.20.1",
    "playwright": "^1.40.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "@types/tmp": "^0.2.6",
    "@vitest/coverage-v8": "^1.0.4",
    "tmp": "^0.2.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {
    "@playwright/test": "^1.40.0"
  },
  "peerDependenciesMeta": {
    "@playwright/test": {
      "optional": true
    }
  },
  "keywords": [
    "e2e",
    "testing",
    "ai",
    "playwright",
    "cypress",
    "automation"
  ],
  "author": "Ebubekir Tabak",
  "license": "MIT",
  "homepage": "https://github.com/ebubekirtabak/riflebird#readme",
  "bugs": {
    "url": "https://github.com/ebubekirtabak/riflebird/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ebubekirtabak/riflebird",
    "directory": "packages/core"
  }
}
